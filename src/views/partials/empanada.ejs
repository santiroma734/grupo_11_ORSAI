<a class="empanada"href="/productos/<%= empanada.id %>">
  <img src="/imgs/productos/<%=empanada.imagen%>" alt="" />
  <div class="info-empanada">
    <div class="nombre-precio">
      <p class="nombre-producto"><%=empanada.nombre%></p>
      <span class="precio">$<%=empanada.precio%></span>
